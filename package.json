    "vue-loader": "^13.7.3",
    "vue-template-compiler": "^2.6.10",
    "vue-test-utils": "^1.0.0-beta.11",
    "webpack": "^3.12.0",
    "webpack-bundle-analyzer": "^3.3.2",
