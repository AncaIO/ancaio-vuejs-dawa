{
  "name": "vue-dawa",
  "version": "1.2.8",
  "description": "A Vue2 component for one-line address autocomplete using Danmarks Adressers WEB API.",
  "main": "./dist/vue-dawa.min.js",
  "files": [
    "dist",
    "src"
  ],
  "config": {
    "prettyName": "Vue DAWA"
  },
  "scripts": {
    "clean": "node build/script.clean.js",
    "dev": "node build/script.dev.js",
    "build": "node build/script.build.js",
    "lint": "eslint --ext .js,.vue src || exit 0",
    "test": "mocha-webpack --webpack-config webpack.config.js --webpack-env=webpack.test.conf --require test/setup.js test/*.spec.js",
    "test:watch": "mocha-webpack --delay --webpack-config webpack.config.js --webpack-env=webpack.test.conf --watch --require test/setup.js test/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AncaIO/vue-dawa.git"
  },
  "keywords": [
